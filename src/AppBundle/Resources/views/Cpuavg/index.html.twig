{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Cpuavg list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Nodeid</th>
                <th>Ctime</th>
                <th>User</th>
                <th>Sys</th>
                <th>Idle</th>
                <th>Iowait</th>
                <th>Soft</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('cpuavg_show', { 'id': entity.id }) }}">{{ entity.nodeid }}</a></td>
                <td>{% if entity.ctime %}{{ entity.ctime|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.user }}</td>
                <td>{{ entity.sys }}</td>
                <td>{{ entity.idle }}</td>
                <td>{{ entity.iowait }}</td>
                <td>{{ entity.soft }}</td>
                <td>{{ entity.id }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('cpuavg_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('cpuavg_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('cpuavg_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
