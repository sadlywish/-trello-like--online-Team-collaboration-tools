{% extends '::base.html.twig' %}

{% block body -%}
    <h1>DiskIo list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Nodeid</th>
                <th>Ctime</th>
                <th>Dev</th>
                <th>Read</th>
                <th>Write</th>
                <th>Readbyte</th>
                <th>Writebyte</th>
                <th>Util</th>
                <th>Await</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('diskio_show', { 'id': entity.id }) }}">{{ entity.nodeid }}</a></td>
                <td>{% if entity.ctime %}{{ entity.ctime|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.dev }}</td>
                <td>{{ entity.read }}</td>
                <td>{{ entity.write }}</td>
                <td>{{ entity.readByte }}</td>
                <td>{{ entity.writeByte }}</td>
                <td>{{ entity.util }}</td>
                <td>{{ entity.await }}</td>
                <td>{{ entity.id }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('diskio_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('diskio_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('diskio_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
